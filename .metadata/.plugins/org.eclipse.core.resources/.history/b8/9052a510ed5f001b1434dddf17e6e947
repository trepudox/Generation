package Exercicios;

import java.util.Scanner;

public class Exe6 {
	
	public static void main(String[] args) {
		Scanner scanner = new Scanner(System.in);
		int x1, y1, x2, y2;
		double d;
		
		System.out.println("Determine a coordenada X de P1: ");
		x1 = scanner.nextInt();
		System.out.println("Determine a coordenada Y de P1: ");
		y1 = scanner.nextInt();
		
		System.out.println("\nDetermine a coordenada X de P2: ");
		x2 = scanner.nextInt();
		System.out.println("Determine a coordenada Y de P2: ");
		y2 = scanner.nextInt();
		
		
		
		scanner.close();
	}
	
}
